    "lint:precommit": "npx pretty-quick --staged && npm run lint:fix && npm run format:fix",
    "setup": "npm install && npx playwright install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SofianeH1/deck-of-cards-e2e-tests.git"
  },
  "keywords": [],
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/SofianeH1/deck-of-cards-e2e-tests/issues"
  },
  "homepage": "https://github.com/SofianeH1/deck-of-cards-e2e-tests#readme",
  "devDependencies": {
    "@playwright/test": "1.56.1",
    "@types/dotenv": "6.1.1",
    "@types/node": "24.8.1",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "dotenv": "16.6.1",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-playwright": "^2.2.2",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jscpd": "^4.0.5",
    "lint-staged": "^16.2.4",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3"
  },
  "directories": {
    "test": "tests"
  },
  "dependencies": {
    "playwright": "1.56.1",
    "playwright-core": "1.56.1",
    "undici-types": "7.14.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
